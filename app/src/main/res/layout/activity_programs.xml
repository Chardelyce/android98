<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#009688"
    tools:context=".programs">

    <ImageView
        android:id="@+id/popp"
        android:layout_width="641dp"
        android:layout_height="826dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.505"
        app:srcCompat="@drawable/menubox" />

    <ImageButton
        android:id="@+id/prompt"
        android:layout_width="278dp"
        android:layout_height="104dp"
        android:layout_marginStart="180dp"
        android:layout_marginBottom="176dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/popp"
        app:layout_constraintStart_toStartOf="@+id/popp"
        app:srcCompat="@drawable/dosy" />

    <ImageButton
        android:id="@+id/prompt2"
        android:layout_width="269dp"
        android:layout_height="129dp"
        android:layout_marginStart="188dp"
        android:layout_marginBottom="228dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/popp"
        app:layout_constraintStart_toStartOf="@+id/popp"
        app:srcCompat="@drawable/netnet" />

    <ImageButton
        android:id="@+id/prompt6"
        android:layout_width="269dp"
        android:layout_height="129dp"
        android:layout_marginStart="168dp"
        android:layout_marginBottom="588dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/popp"
        app:layout_constraintStart_toStartOf="@+id/popp"
        app:srcCompat="@drawable/uppies" />

    <ImageButton
        android:id="@+id/nono"
        android:layout_width="269dp"
        android:layout_height="129dp"
        android:layout_marginStart="164dp"
        android:layout_marginBottom="400dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/popp"
        app:layout_constraintStart_toStartOf="@+id/popp"
        app:srcCompat="@drawable/notey" />

    <ImageButton
        android:id="@+id/prompt7"
        android:layout_width="269dp"
        android:layout_height="129dp"
        android:layout_marginStart="180dp"
        android:layout_marginBottom="528dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/popp"
        app:layout_constraintStart_toStartOf="@+id/popp"
        app:srcCompat="@drawable/welcy" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="160dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.043"
        app:srcCompat="@drawable/toolbars" />

    <TextView
        android:id="@+id/date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="500dp"
        android:layout_marginEnd="4dp"
        android:textColor="#000000"
        android:textSize="6sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <ImageButton
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="235dp"
        android:background="@null"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.977"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:srcCompat="@drawable/startmenu" />

    <ImageButton
        android:id="@+id/prompt5"
        android:layout_width="321dp"
        android:layout_height="204dp"
        android:layout_marginStart="156dp"
        android:layout_marginBottom="256dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/popp"
        app:layout_constraintStart_toStartOf="@+id/popp"
        app:srcCompat="@drawable/blob" />

</androidx.constraintlayout.widget.ConstraintLayout>